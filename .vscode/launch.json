  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Remote Debugger",
      "address": "127.0.0.1",
      "port": 9229,
      "localRoot": "${workspaceFolder}/apps/server/src",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node-terminal",
      "name": "JavaScript Debug Terminal",
      "request": "launch"
    }
  ]
}